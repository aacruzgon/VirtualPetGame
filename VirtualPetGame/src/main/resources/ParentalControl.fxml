<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="parentalControlRoot" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
    <!-- Background Image -->
    <ImageView fx:id="backgroundImage" pickOnBounds="true" preserveRatio="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <image>
            <Image url="@Sprites/sky.png" />
        </image>
    </ImageView>

    <!-- HBox for Title -->
    <HBox fx:id="settingsTitleContainer" alignment="CENTER" layoutX="13.0" prefWidth="1779.0" spacing="10.0" AnchorPane.leftAnchor="13.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0">
        <children>
            <AnchorPane fx:id="virtual">
                <children>
                    <ImageView fitHeight="116.0" fitWidth="740.0" layoutX="-92.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@Sprites/settings.png" />
                        </image>
                    </ImageView>
                </children>
            </AnchorPane>
        </children>
    </HBox>

</AnchorPane>
